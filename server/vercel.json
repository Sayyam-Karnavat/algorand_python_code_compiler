{
    "version": 2,
    "builds": [
      {
        "src": "Dockerfile",
        "use": "@vercel/docker"
      }
    ],
    "routes": [
      { 
        "src": "/deploy", 
        "dest": "/deploy",
        "methods": ["POST"]
      },
      { 
        "src": "/run", 
        "dest": "/run",
        "methods": ["POST"]
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }