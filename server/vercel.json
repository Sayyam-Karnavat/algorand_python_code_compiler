{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "buildCommand": "pip install algokit && export PATH=$PATH:$HOME/.local/bin",
                "pythonVersion": "3.12"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ]
}